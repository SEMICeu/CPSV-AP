    {
      "@id": "https://semiceu.github.io/shacl/cpsv-ap_en#DatasetShape",
      "@type": "sh:NodeShape",
      "sh:closed": false,
      "sh:property": [
        {
          "sh:class": "http://www.w3.org/1999/02/22-rdf-syntax-ns#langString",
          "sh:definition": {
            "en": "This property contains a free-text account of the Dataset. This property can be repeated for parallel language versions of the description."
          },
          "sh:minCount": "1",
          "sh:name": {
            "en": "description"
          },
          "sh:path": "http://purl.org/dc/terms/description"
        },
        {
          "sh:class": "http://www.w3.org/1999/02/22-rdf-syntax-ns#langString",
          "sh:definition": {
            "en": "This property contains the main identifier for the Dataset, e.g. the URI or other unique identifier in the context of the Catalogue."
          },
          "sh:name": {
            "en": "identifier"
          },
          "sh:path": "http://purl.org/dc/terms/identifier"
        },
        {
          "sh:class": "http://fixme.com#Document",
          "sh:definition": {
            "en": "This property refers to a web page that provides access to the Dataset, its Distributions and/or additional information."
          },
          "sh:name": {
            "en": "landing page"
          },
          "sh:path": "http://www.w3.org/ns/dcat#landingPage"
        },
        {
          "sh:class": "http://xmlns.com/foaf/0.1/Agent",
          "sh:definition": {
            "en": "This property represents the Publisher of the Public Service Dataset, i.e. an entity  (organisation) responsible for making the Public Service Dataset available."
          },
          "sh:maxCount": "1",
          "sh:minCount": "1",
          "sh:name": {
            "en": "publisher"
          },
          "sh:path": "http://purl.org/dc/terms/publisher"
        },
        {
          "sh:class": "http://www.w3.org/1999/02/22-rdf-syntax-ns#langString",
          "sh:definition": {
            "en": "This property contains a name given to the Dataset."
          },
          "sh:minCount": "1",
          "sh:name": {
            "en": "title"
          },
          "sh:path": "http://purl.org/dc/terms/title"
        }
      ],
      "sh:targetClass": "http://www.w3.org/ns/dcat#Dataset"
    },
    {
      "@id": "https://semiceu.github.io/shacl/cpsv-ap_en#AddressShape",
      "@type": "sh:NodeShape",
      "sh:closed": false,
      "sh:property": [
        {
          "sh:class": "http://www.w3.org/1999/02/22-rdf-syntax-ns#langString",
          "sh:definition": {
            "en": "The name or names of a geographic area or locality that groups a number of addressable objects for addressing purposes, without being an administrative unit."
          },
          "sh:maxCount": "1",
          "sh:minCount": "1",
          "sh:name": {
            "en": "address area"
          },
          "sh:path": "http://www.w3.org/ns/locn#addressArea"
        },
        {
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string",
          "sh:definition": {
            "en": "A globally unique identifier for each instance of an Address."
          },
          "sh:maxCount": "1",
          "sh:minCount": "1",
          "sh:name": {
            "en": "address ID"
          },
          "sh:path": "http://www.w3.org/ns/locn#addressID"
        },
        {
          "sh:class": "http://www.w3.org/2004/02/skos/core#Concept",
          "sh:definition": {
            "en": "The name or names of a unit of administration where a Member State has and/or exercises jurisdictional rights, for local, regional and national governance. Level 1 refers to the uppermost administrative unit for the address, almost always a country."
          },
          "sh:maxCount": "1",
          "sh:minCount": "1",
          "sh:name": {
            "en": "administrative unit level 1 (country)"
          },
          "sh:path": "http://www.w3.org/ns/locn#adminUnitL1"
        },
        {
          "sh:class": "http://www.w3.org/1999/02/22-rdf-syntax-ns#langString",
          "sh:definition": {
            "en": "The name or names of a unit of administration where a Member State has and/or exercises jurisdictional rights, for local, regional and national governance. Level 2 referst to the region of the address, usually a county, state or other such area that typically encompasses several localities."
          },
          "sh:maxCount": "1",
          "sh:minCount": "1",
          "sh:name": {
            "en": "administrative unit level 2 (country/region/state)"
          },
          "sh:path": "http://www.w3.org/ns/locn#adminUnitL2"
        },
        {
          "sh:class": "http://www.w3.org/1999/02/22-rdf-syntax-ns#langString",
          "sh:definition": {
            "en": "The complete address written as a formatted string."
          },
          "sh:maxCount": "1",
          "sh:minCount": "1",
          "sh:name": {
            "en": "full address"
          },
          "sh:path": "http://www.w3.org/ns/locn#fullAddress"
        },
        {
          "sh:datatype": "http://www.w3.org/2001/XMLSchema#string",
          "sh:definition": {
            "en": "A number or a sequence of characters which allows a user or an application to interpret, parse and format the locator within the relevant scope. A locator may include more locator designators."
          },
          "sh:maxCount": "1",
          "sh:minCount": "1",
          "sh:name": {
            "en": "locator designator"
          },
          "sh:path": "http://www.w3.org/ns/locn#locatorDesignator"
        },
        {
          "sh:class": "http://www.w3.org/1999/02/22-rdf-syntax-ns#langString",
          "sh:definition": {
            "en": "Proper noun(s) applied to the real world entity identified by the locator."
          },
          "sh:maxCount": "1",
          "sh:minCount": "1",
          "sh:name": {
            "en": "locator name"
          },
          "sh:path": "http://www.w3.org/ns/locn#locatorName"
        },
        {
