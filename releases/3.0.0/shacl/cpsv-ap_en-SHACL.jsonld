    {
      "@id": "https://semiceu.github.io/shacl/cpsv-ap_en#LegalResourceShape",
      "@type": "sh:NodeShape",
      "sh:closed": false,
      "sh:property": [
        {
          "sh:class": "http://data.europa.eu/eli/ontology#LegalResource",
          "sh:definition": {
            "en": "This property represents another instance of the Legal Resource class that is related to the particular Legal Resource being described."
          },
          "sh:name": {
            "en": "related"
          },
          "sh:path": "http://purl.org/dc/terms/relation"
        }
