    shacl:name "identifier"@en;
    shacl:path dc:identifier
  ], [
    shacl:datatype rdf:langString;
    shacl:description "This property represents the Name of the Agent."@en;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "name"@en;
    shacl:path dc:title
